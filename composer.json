{
    "name": "stepthefkup/api-token",
    "description": "Tools to handle API tokens like a pro",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "eoneopay/utils": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "ext-openssl": "^7.1",
        "firebase/php-jwt": "^5.0",
        "illuminate/support": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "StepTheFkUp\\ApiToken\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StepTheFkUp\\ApiToken\\Tests\\": "tests"
        }
    },
    "suggest": {
        "firebase/php-jwt": "To use the Firebase JWT driver to decode/encode tokens"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
