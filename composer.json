{
    "name": "stepthefkup/apitoken",
    "description": "Tools to handle API tokens like a pro",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "eoneopay/utils": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "StepTheFkUp\\ApiToken\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StepTheFkUp\\ApiToken\\Tests\\": "tests"
        }
    },
    "suggest": {
        "firebase/php-jwt": "To use the JWT token resolver"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
